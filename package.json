{
  "name": "redux-hom",
  "version": "0.1.3",
  "description": "redux HigherOrderMiddleware",
  "main": "lib/redux-hom.js",
  "module": "es/redux-hom.js",
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "test": "cross-env BABEL_ENV=commonjs mocha --compilers js:babel-core/register --reporter spec test/*.js",
    "build:commonjs": "cross-env NODE_ENV=cjs rollup -c -o lib/redux-hom.js",
    "build:es": "cross-env BABEL_ENV=es NODE_ENV=es rollup -c -o es/redux-hom.js",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -o dist/redux-hom.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -o dist/redux-hom.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YaaMe/redux-hom.git"
  },
  "keywords": [
    "redux",
    "hom",
    "middleware",
    "higherOrderMiddleware",
    "functional"
  ],
  "author": "yaame",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YaaMe/redux-hom/issues"
  },
  "homepage": "https://github.com/YaaMe/redux-hom#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "cross-env": "^1.0.7",
    "chai": "^3.2.0",
    "eslint": "^1.10.2",
    "eslint-config-airbnb": "1.0.2",
    "eslint-plugin-react": "^4.1.0",
    "mocha": "^2.2.5",
    "redux": "^3.4.0",
    "rimraf": "^2.5.2",
    "rollup": "^0.58.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "typescript": "^1.8.10",
    "typescript-definition-tester": "0.0.4"
  }
}
